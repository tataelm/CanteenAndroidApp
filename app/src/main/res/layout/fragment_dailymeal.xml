<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_dailyMeal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dailymeal.DailyMealFragment">

    <LinearLayout
        android:id="@+id/elementFood_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />


        <TextView
            android:id="@+id/textView_food1"
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/element_foodtype_margintopbottom"
            android:layout_marginBottom="@dimen/element_foodtype_margintopbottom"
            android:layout_marginStart="@dimen/element_foodtype_marginstartend"
            android:layout_marginEnd="@dimen/element_foodtype_marginstartend"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:text="TARHANA" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>